{
  "version": 2,
  "projects": {
    "auth-api": {
      "root": "packages/auth-api",
      "sourceRoot": "packages/auth-api/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/auth-api",
            "tsConfig": "packages/auth-api/tsconfig.lib.json",
            "project": "packages/auth-api/package.json",
            "entryFile": "packages/auth-api/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "packages/auth-api/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/auth-api/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/auth-api"],
          "options": {
            "jestConfig": "packages/auth-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "auth-hook": {
      "root": "packages/auth-hook",
      "sourceRoot": "packages/auth-hook/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/auth-hook",
            "tsConfig": "packages/auth-hook/tsconfig.lib.json",
            "project": "packages/auth-hook/package.json",
            "entryFile": "packages/auth-hook/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "packages/auth-hook/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/auth-hook/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/auth-hook"],
          "options": {
            "jestConfig": "packages/auth-hook/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core": {
      "root": "packages/core",
      "sourceRoot": "packages/core/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/core",
            "tsConfig": "packages/core/tsconfig.lib.json",
            "project": "packages/core/package.json",
            "entryFile": "packages/core/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "packages/core/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/core/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/core"],
          "options": {
            "jestConfig": "packages/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-react-sample": {
      "root": "packages/demo/react-sample",
      "sourceRoot": "packages/demo/react-sample/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/demo/react-sample",
            "index": "packages/demo/react-sample/src/index.html",
            "main": "packages/demo/react-sample/src/main.tsx",
            "polyfills": "packages/demo/react-sample/src/polyfills.ts",
            "tsConfig": "packages/demo/react-sample/tsconfig.app.json",
            "assets": [
              "packages/demo/react-sample/src/favicon.ico",
              "packages/demo/react-sample/src/assets"
            ],
            "styles": ["packages/demo/react-sample/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/demo/react-sample/src/environments/environment.ts",
                  "with": "packages/demo/react-sample/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "demo-react-sample:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "demo-react-sample:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": [
              "packages/demo/react-sample/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/demo/react-sample"],
          "options": {
            "jestConfig": "packages/demo/react-sample/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-react-sample-e2e": {
      "root": "packages/demo/react-sample-e2e",
      "sourceRoot": "packages/demo/react-sample-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "packages/demo/react-sample-e2e/cypress.json",
            "devServerTarget": "demo-react-sample:serve",
            "tsConfig": "packages/demo/react-sample-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo-react-sample:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/demo/react-sample-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "demo-react-sample"
}
